        {
            "type": "java",
            "name": "Launch Encode Steganography",
            "request": "launch",
            "mainClass": "com.paulo.steganography.Steganography",
            "projectName": "Steganography",
            "args": ["e", "~/Pictures/Steganography/horizon.jpeg", "~/Desktop/audioJaque.mp3", "~/Desktop/"]
        },
        {
            "type": "java",
            "name": "Launch Decode Steganography",
            "request": "launch",
            "mainClass": "com.paulo.steganography.Steganography",
            "projectName": "Steganography",
            "args": ["d", "~/Downloads/_horizon(1).jpeg", "~/Desktop/"]
        }
    ]
